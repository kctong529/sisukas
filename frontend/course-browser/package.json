{
  "name": "course-browser",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --reporter=verbose",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "compile-metadata": "node compile-metadata.js",
    "dev:mkcert": "mkcert -install && mkcert localhost 127.0.0.1 ::1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@modyfi/vite-plugin-yaml": "1.1.1",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@tsconfig/svelte": "5.0.5",
    "@types/node": "24.10.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "svelte": "5.43.2",
    "svelte-check": "4.3.3",
    "svelte-eslint-parser": "^1.4.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "7.1.12",
    "vitest": "^4.0.16",
    "yaml": "2.8.1"
  },
  "overrides": {
    "js-yaml": "4.1.1"
  },
  "dependencies": {
    "better-auth": "^1.4.10"
  }
}
